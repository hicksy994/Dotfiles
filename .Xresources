!rofi

rofi.color-enabled: true
rofi.color-window: #282828, #282828, #282828
rofi.color-normal: #282828, #ebdbb2, #282828, #504945, #fdf4c1
! rofi.color-window: #2f343f, #2f343f, #2f343f
! rofi.color-normal: #2f343f, #ffffff, #2f343f, #268bd2, #ffffff
! rofi.color-active: #2f343f, #268bd2, #2f343f, #268bd2, #205171
rofi.font: Droid Sans 14
rofi.hide-scrollbar: true
rofi.lines: 3
rofi.eh: 2
rofi.width: 30
rofi.padding:0
rofi.bw: 0
rofi.show-match: false

!urxvt

URxvt.font:                xft:Hack:pixelsize=14:antialias=true
URxvt.boldFont:            xft:Hack:pixelsize=14:bold:antialias=true
URxvt.italicFont:          xft:Hack:pixelsize=14:italic:antialias=true
URxvt.boldItalicFont:      xft:Hack:pixelsize=14:bold:italic:antialias=true
URxvt.letterSpace:         -1

!standard dark theme

/* URxvt*background: #161719 */
/* URxvt*foreground: #DEDEDE */

URxvt*colorUL: #86a2b0

! black
! URxvt*color0  : #2E3436
! URxvt*color8  : #555753
! ! red
! URxvt*color1  : #CC0000
! URxvt*color9  : #EF2929
! ! green
! URxvt*color2  : #4E9A06
! URxvt*color10 : #8AE234
! ! yellow
! URxvt*color3  : #C4A000
! URxvt*color11 : #FCE94F
! ! blue
! URxvt*color4  : #3465A4
! URxvt*color12 : #729FCF
! ! magenta
! URxvt*color5  : #75507B
! URxvt*color13 : #AD7FA8
! ! cyan
! URxvt*color6  : #06989A
! URxvt*color14 : #34E2E2
! ! white
! URxvt*color7  : #D3D7CF
! URxvt*color15 : #EEEEEC

! Gruvbox Theme
*background: #282828
*foreground: #fdf4c1
! Black + DarkGrey
*color0:  #282828
*color8:  #928374
! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934
! DarkGreen + Green
*color2:  #98971a
*color10: #b8bb26
! DarkYellow + Yellow
*color3:  #d79921
*color11: #fabd2f
! DarkBlue + Blue
*color4:  #458588
*color12: #83a598
! DarkMagenta + Magenta
*color5:  #b16286
*color13: #d3869b
! DarkCyan + Cyan
*color6:  #689d6a
*color14: #8ec07c
! LightGrey + White
*color7:  #a89984
*color15: #ebdbb2

URxvt.urlLauncher:      /usr/bin/google-chrome-stable
URxvt.matcher.button:   1 


URxvt.internalBorder:      0
URxvt.externalBorder:      0

URxvt.scrollBar:           false

URxvt.perl-ext-common:     matcher,resize-font,foo

URxvt.keysym.F11:          perl:fullscreen:switch

URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger:  C-Up

URxvt.keysym.M-Delete:     perl:matcher:list
URxvt.colorUL:             #7780a1
