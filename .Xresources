URxvt*background:                     #282c34
URxvt*foreground:                     #d1d4e0
URxvt*cursorColor:                    #51afef
URxvt*scrollColor:                    #d1d4e0
URxvt*highlightColor:                 #42444a
! URxvt*highlightTextColor:             #959ba5


URxvt.urlLauncher:      /usr/bin/chromium
URxvt.matcher.button:   1 

URxvt.internalBorder:      0
URxvt.externalBorder:      0

URxvt.scrollBar:           false

URxvt.perl-ext-common:     matcher,resize-font,foo

URxvt.keysym.F11:          perl:fullscreen:switch

URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger:  C-Up

URxvt.keysym.M-Delete:     perl:matcher:list
URxvt.colorUL:             #7780a1
