!urxvt

! URxvt.font:                xft:Roboto Mono for Powerline:pixelsize=14:antialias=true
! URxvt.boldFont:            xft:Roboto Mono for Powerline:pixelsize=14:bold:antialias=true
! URxvt.italicFont:          xft:Roboto Mono for Powerline:pixelsize=14:italic:antialias=true
! URxvt.boldItalicFont:      xft:Roboto Mono for Powerline:pixelsize=14:bold:italic:antialias=true
URxvt.font:                xft:Iosevka:pixelsize=16:antialias=true
URxvt.boldFont:            xft:Iosevka:pixelsize=16:bold:antialias=true
URxvt.italicFont:          xft:Iosevka:pixelsize=16:italic:antialias=true
URxvt.boldItalicFont:      xft:Iosevka:pixelsize=16:bold:italic:antialias=true
/* URxvt.letterSpace:         -1 */
Xft.antialias:  1
Xft.hinting:    1
Xft.hintstyle:  hintfull
Xft.lcdfilter:  lcddefault
Xft.rgba:       rgb

